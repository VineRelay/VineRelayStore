{
  "name": "vinerelay",
  "version": "0.0.1",
  "scripts": {
    "migrations": "babel-node internals/migrations/runMigrations",
    "migrations:create": "babel-node internals/migrations/createMigration",
    "migrations:rollback": "babel-node internals/migrations/rollbackMigration",
    "migrations:run": "babel-node internals/migrations/runMigrations",
    "start:graphql": "concurrently \"npm run watcher:graphql\" \"npm run serve:graphql\"",
    "start:app": "concurrently \"npm run watcher:app\" \"npm run serve:app\"",
    "watcher:graphql": "nodemon --watch server/graphql/**/*.js --exec \"npm run update-graphql-schema\"",
    "watcher:app": "nodemon --watch app/components/**/*.js --exec \"npm run update-relay\"",
    "serve:graphql": "nodemon --watch server --exec \"babel-node server graphql\"",
    "serve:app": "babel-node server app",
    "build:prod": "cross-env NODE_ENV=production npm run clean && npm run update-graphql-schema && npm run update-relay && npm run build:app && npm run build:server",
    "clean": "rimraf build && rimraf build-server",
    "update-graphql-schema": "babel-node internals/updateSchema.js",
    "update-relay": "relay-compiler --src ./app --schema server/graphql/__generated__/schema.graphql",
    "build:app": "webpack --config internals/webpack/webpack.prod.config.js --color -p --progress",
    "build:server": "./node_modules/.bin/babel ./server --out-dir ./build-server",
    "start:prod": "cross-env NODE_ENV=production node build-server",
    "heroku-postbuild": "npm run build:prod"
  },
  "engines": {
    "node": "~6.9.0",
    "npm": "3.10.8"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-relay": "^1.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.4",
    "eventsource-polyfill": "^0.9.6",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "lodash-webpack-plugin": "^0.11.4",
    "nodemon": "^1.11.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.4.2",
    "relay-compiler": "^1.1.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "chalk": "^1.1.3",
    "compression": "^1.7.0",
    "connect-history-api-fallback": "^1.3.0",
    "cors": "^2.8.3",
    "cross-env": "^5.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "flux": "^3.1.2",
    "graphql": "^0.10.3",
    "graphql-relay": "^0.5.2",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.18.3",
    "moment": "^2.18.1",
    "mongodb-migrations": "^0.8.5",
    "mongoose": "^4.10.7",
    "mongoose-unique-validator": "^1.0.5",
    "mongoose-validator": "^1.3.2",
    "password-hash-and-salt": "^0.1.4",
    "pretty-error": "^2.1.1",
    "q": "^1.5.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-icon-base": "^2.1.0",
    "react-relay": "^1.0.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "relay-runtime": "^1.0.0",
    "sanitize.css": "^5.0.0",
    "styled-components": "^2.1.0",
    "whatwg-fetch": "^2.0.3"
  }
}
